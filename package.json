{
  "name": "icd10",
  "version": "0.0.1",
  "description": "Package that goal is to help working with ICD Classificantion created by WHO",
  "main": "dist/src/icd.js",
  "types": "dist/src/icd.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "node ./dist/src/test.js",
    "generateXSD": "perl generators/dtd2xsd.pl data/icdClaML2016ens/ClaML.dtd >> ClaML.xsd",
    "generateJSON": "node ./dist/generators/xml2jsonFile.js ./",
    "prepublish": "tsc",
    "postinstall": "node ./dist/generators/xml2jsonFile.js"
  },
  "keywords": [
    "icd",
    "icd10",
    "who",
    "classification"
  ],
  "author": "revolve.pro",
  "license": "MIT",
  "dependencies": {
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@types/xml2js": "^0.4.5",
    "typescript": "^3.7.5"
  },
  "icd10": {
    "xmlPath": "data/icdClaML2016ens/icdClaML2016ens.xml"
  }
}
